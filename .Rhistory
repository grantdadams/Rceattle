lwd = lwd,
col = line_col[k]
) # Median
# Ptarget and plimit
abline(h= ptarget[spp[j]], lwd = lwd/2, col = "blue")
abline(h= plimit[spp[j]], lwd = lwd/2, col = "red")
}
}
