journal = {Fisheries Research},
keywords = {Fisheries management,Gulf of Alaska,Management strategy evaluation,Predation,Walleye pollock},
number = {1-2},
pages = {98--114},
title = {{Incorporating ecosystem forcing through predation into a management strategy evaluation for the Gulf of Alaska walleye pollock (Theragra chalcogramma) fishery}},
volume = {102},
