keywords = {7600 sand point way,a,alaska fisheries science center,and p,b,hollowed,ianelli,j,livingston,n,national marine fisheries service,ne,predation,seattle,stock assessment,uncertainty,usa,wa 98115,walleye pollock},
month = {apr},
number = {2},
pages = {279--293},
pmid = {200000315235},
title = {{Including predation mortality in stock assessments: a case study for Gulf of Alaska walleye pollock}},
url = {https://academic.oup.com/icesjms/article-lookup/doi/10.1006/jmsc.1999.0637},
